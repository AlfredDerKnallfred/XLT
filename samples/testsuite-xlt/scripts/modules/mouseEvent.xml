<?xml version="1.0" encoding="UTF-8"?>
<scriptmodule xmlns="http://xlt.xceptance.com/xlt-script/2" version="6">
  <module name="modules.Open_ExamplePage"/>
  <action name="mouseOver"/>
  <command name="mouseOver" target="id=mouseevent_a"/>
  <command name="assertText" target="id=cc_mouseover_head" value="mouseover (mouseevent_a)"/>
  <action name="mouseDown"/>
  <command name="mouseDown" target="xpath=id('mouseevent')/a"/>
  <command name="assertText" target="id=cc_mousedown_head" value="mousedown (mouseevent_a)"/>
  <action name="mouseUp"/>
  <command name="mouseUp" target="name=mouseevent_a"/>
  <command name="assertText" target="id=cc_mouseup_head" value="mouseup (mouseevent_a)"/>
  <action name="mouseOut"/>
  <command name="mouseOut" target="link=mouseevent_a"/>
  <command name="assertText" target="id=cc_mouseout_head" value="mouseout (mouseevent_a)"/>
  <action name="mouseOver"/>
  <command name="click" target="id=cc_clear_button"/>
  <command name="mouseOver" target="dom=document.getElementById('mouseevent_a')"/>
  <command name="assertText" target="id=cc_mouseover_head" value="mouseover (mouseevent_a)"/>
  <action name="mouseDownAt"/>
  <command name="click" target="id=cc_clear_button"/>
  <command name="mouseDownAt" target="css=#mouseevent_a" value="5,3"/>
  <command name="assertText" target="id=cc_mousedown_head" value="mousedown (mouseevent_a)"/>
  <action name="mouseMoveAt"/>
  <command name="mouseMoveAt" target="id=mouseevent_a" value="5,5"/>
  <command name="assertText" target="id=cc_mousemove_head" value="mousemove (mouseevent_a)"/>
  <action name="mouseUpAt"/>
  <command name="mouseUpAt" target="id=mouseevent_a" value="20,3"/>
  <command name="assertText" target="id=cc_mouseup_head" value="mouseup (mouseevent_a)"/>
  <action name="mouseMove"/>
  <command name="click" target="id=cc_clear_button"/>
  <command name="mouseMove" target="id=mouseevent_a"/>
  <command name="assertText" target="id=cc_mousemove_head" value="mousemove (mouseevent_a)"/>
</scriptmodule>